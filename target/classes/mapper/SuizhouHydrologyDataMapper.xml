<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hxkj.mybatis.mapper.SuizhouHydrologyDataMapper">

    <select id="getAllSuizhouData" resultType="com.hxkj.mybatis.entity.SuizhouHydrologyData">
        select *
        from suizhou_hydrology_data where datetime = '2022-05-11 05:00:00';
    </select>

    <insert id="insert" parameterType="com.hxkj.mybatis.entity.StPptnR">
        insert into suizhou_hydrology_data
            (station_id_c,datetime, drp_densty, intv_densty, pdr_densty, dyp_densty, wth_densty)
        values
            (#{stcd},cast(#{tm} as timestamp without time zone),#{drp},#{intv},#{pdr},#{dyp},#{wth});
    </insert>

</mapper>
